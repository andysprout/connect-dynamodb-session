  },
  "devDependencies": {
    "babel-cli": "6.7.5",
    "babel-core": "6.7.6",
    "babel-istanbul": "0.7.0",
    "babel-preset-es2015": "6.6.0",
    "blue-tape": "0.2.0",
    "codeclimate-test-reporter": "0.3.1",
    "eslint": "2.7.0",
    "eslint-config-airbnb": "7.0.0",
